
<%= link_to 'New District',  new_district_path %>
<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>InProvince</th>
    <th colspan="3"></th>
  </tr>
 
  <% @districts.each do |district| %>
    <tr>
      <td><%= district.id %></td>
      <td><%= district.dis_name %></td>
       <td><%= district.province_id %></td>
      <td><%= link_to 'Show', district_path(district) %></td>
      <td><%= link_to 'Edit', edit_district_path(district) %></td>
      <td><%= link_to 'Destroy', district_path(district), method: :delete, data: {confirm: ' Are you sure?'} %></td>
    </tr>
  <% end %>
</table>